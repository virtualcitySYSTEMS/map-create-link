{
  "name": "@vcmap/create-link",
  "version": "1.0.1",
  "description": "Plugin to copy app state to URL.",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "pack": "vcmplugin pack",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix"
  },
  "author": "Ben Kuster <bkuster@vc.systems>",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "./src/index.js",
    "./dist": "./dist/index.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "eslintConfig": {
    "root": true,
    "extends": "@vcsuite/eslint-config/vue"
  },
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "peerDependencies": {
    "@vcmap/ui": "^5.0.0-rc.30",
    "vue": "~2.7.3",
    "vuetify": "~2.6.7"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^2.1.11",
    "@vcsuite/eslint-config": "^3.0.3"
  }
}
